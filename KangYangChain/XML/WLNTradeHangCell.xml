<UIView layout="" attr="@:css/colors">


    <UIView layout="margin:5" attr="bgColor:white,borderRadius:7,@:css/shadows">
    
    
        <UIView layout="flexDirection:row,justifyContent:space-between,alignItems:center" attr="">
            <UILabel name="typeLab" layout="margin:13" attr="font:bold|15,color:green,linesNum:0,text:--"/>
            <FlexTouchView onPress="cancelOrderAction" layout="" attr="underlayColor:darkGray">
                <UILabel name="" layout="margin:13" attr="fontSize:14,color:orange,linesNum:0,text:取消挂单"/>
            </FlexTouchView>
        </UIView>

        <UILabel name="" layout="height:1" attr="fontSize:14,@:css/colors,linesNum:0,text:"/>
        
        <UIView layout="flexDirection:row" attr="">
            <UIView layout="" attr="">
                <UILabel name="numLab" layout="margin:13,marginBottom:0" attr="fontSize:14,color:#333333,linesNum:0,text:数量:"/>
                <UILabel name="priceLab" layout="margin:13,marginBottom:0" attr="fontSize:14,color:#333333,linesNum:0,text:挂单价:"/>
                <UILabel name="maxlineLab" layout="margin:13" attr="fontSize:14,color:#333333,linesNum:0,text:止盈:"/>
            </UIView>

            <UIView layout="" attr="">

                <UILabel name="amountLab" layout="margin:13,marginBottom:0" attr="fontSize:14,color:#333333,linesNum:0,text:冻结:"/>
                <UILabel name="minlineLab" layout="margin:13,marginBottom:0" attr="fontSize:14,color:#333333,linesNum:0,text:止损:"/>

            </UIView>


        </UIView>

        <UILabel name="" layout="height:1" attr="fontSize:14,@:css/colors,linesNum:0,text:"/>

        <UIView layout="alignItems:flex-end,flex:1" attr="">

            <UILabel name="timeLab" layout="margin:13" attr="fontSize:14,color:#333333,linesNum:0,text:--"/>

        </UIView>

    </UIView>



</UIView>


